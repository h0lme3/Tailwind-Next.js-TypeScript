    "autoprefixer": "^10.4.14",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.3.2",
